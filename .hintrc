{
    "extends": ["development"],
    "hints": {
        "axe/text-alternatives": [
            "default",
            {
                "image-alt": "off",
                "frame-title": "off"
            }
        ],
        "no-inline-styles": "off",
        "disown-opener": "off",
        "typescript-config/strict": "off",
        "compat-api/css": [
            "default",
            {
                "ignore": [
                    "text-size-adjust",
                    "scrollbar-color",
                    "scrollbar-width",
                    "scrollbar-width: thin"
                ]
            }
        ],
        "axe/name-role-value": [
            "default",
            {
                "button-name": "off"
            }
        ],
        "axe/forms": [
            "default",
            {
                "label": "off"
            }
        ]
    }
}
